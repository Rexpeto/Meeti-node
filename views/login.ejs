<main class="contenedor contenedor-formularios">
    <h1><%- pagina %></h1>
    <form method="POST" class="default-form">
        <div class="campo">
            <label for="email">Correo:</label>
            <input type="email" name="email" id="email" placeholder="Coloque su email">
        </div>

        <div class="campo">
            <label for="password">Contraseña:</label>
            <input type="password" name="password" id="password" placeholder="Coloque su contraseña">
        </div>

        <div class="campo enviar">
            <input type="submit" value="Iniciar Sesión" class="btn btn-rosa">
        </div>
    </form>
</main>